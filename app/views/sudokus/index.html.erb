<h1>Listing sudokus</h1>

<table>
  <tr>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sudokus.each do |sudoku| %>
  <tr>
    <td><%= sudoku.field11 %></td>
    <td><%= sudoku.field12 %></td>
    <td><%= sudoku.field13 %></td>
  </tr> 
  <tr>
    <td><%= sudoku.field21 %></td>
    <td><%= sudoku.field22 %></td>
    <td><%= sudoku.field23 %></td>
  </tr> 
  <tr>
    <td><%= sudoku.field31 %></td>
    <td><%= sudoku.field32 %></td>
    <td><%= sudoku.field33 %></td>
  </tr>
  <tr>
    <td><%= link_to 'Show', sudoku %></td>
    <td><%= link_to 'Edit', edit_sudoku_path(sudoku) %></td>
    <td><%= link_to 'Destroy', sudoku, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Sudoku', new_sudoku_path %>